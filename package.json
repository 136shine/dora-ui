{
  "name": "dora-ui",
  "version": "0.0.1",
  "description": "A React.js Mobile UI Library",
  "repository": "https://github.com/worldzhao/dora-ui",
  "homepage": "https://dora-ui.netlify.com/",
  "license": "MIT",
  "private": false,
  "authors": {
    "name": "worldzhao",
    "email": "1007326924@qq.com"
  },
  "files": [
    "lib",
    "es",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "keywords": [
    "dora",
    "dora-ui",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend",
    "mobile"
  ],
  "tags": [
    "dora",
    "dora-ui",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend",
    "mobile"
  ],
  "sideEffects": [
    "dist/*",
    "lib/*",
    "es/*",
    "*.less"
  ],
  "scripts": {
    "start": "yarn dev",
    "dev": "docz dev",
    "clean": "rimraf lib es dist",
    "commit": "git cz",
    "type-check": "tsc --noEmit",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prebuild:doc": "yarn build:changelog",
    "build:doc": "docz build",
    "build:types": "tsc --emitDeclarationOnly --outDir lib && cpr lib es",
    "prebuild:comps": "yarn build:types",
    "build:comps": "gulp",
    "prebuild": "yarn clean",
    "build": "run-p build:doc build:comps"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@semantic-release/changelog": "3.0.2",
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.1",
    "@types/warning": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.1.1",
    "commitlint": "^7.6.1",
    "conventional-changelog": "^3.1.8",
    "conventional-changelog-cli": "^2.0.21",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "1.2.0",
    "docz-plugin-netlify": "1.1.4",
    "docz-theme-default": "1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^2.3.0",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "0.7.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "4.8",
    "react-spinners": "0.5.13",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "through2": "^3.0.1",
    "typescript": "3.3.3",
    "webpack-merge": "4.2.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "lint-staged": {
    "components/**/*.tsx": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.6",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react-transition-group": "^4.0.1",
    "warning": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
