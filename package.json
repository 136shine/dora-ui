{
  "name": "dora-ui",
  "version": "0.0.9",
  "description": "A React.js Mobile UI Library",
  "repository": "https://github.com/worldzhao/dora-ui",
  "homepage": "https://dora-ui.netlify.com/",
  "license": "MIT",
  "private": false,
  "authors": {
    "name": "worldzhao",
    "email": "1007326924@qq.com"
  },
  "files": [
    "lib",
    "es",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "keywords": [
    "dora",
    "dora-ui",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend",
    "mobile"
  ],
  "tags": [
    "dora",
    "dora-ui",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend",
    "mobile"
  ],
  "scripts": {
    "start": "run-s dev",
    "dev": "node ./scripts/start",
    "clean:comps": "rimraf lib es dist",
    "clean:doc": "rimraf site",
    "type-check": "tsc --noEmit",
    "build:doc": "run-s clean:doc && node ./scripts/build-doc",
    "build:types": "tsc --emitDeclarationOnly --outDir lib && cpr lib es",
    "build:comps": "gulp",
    "build": "run-s clean:* build:types && run-p build:doc build:comps",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^3.0.4",
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.1",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "address": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-import": "^1.12.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.25",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "doc-scripts": "^1.8.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^2.3.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8",
    "react-spinners": "^0.5.13",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.19",
    "standard-version": "^7.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "through2": "^3.0.1",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "react": ">=16.6.0",
    "react-dom": ">=16.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.0.1",
    "warning": "^4.0.3"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  }
}
